<h1>Books</h1>
<table>
  <tr>
    <th>Title</th>
    <th>Body</th>
  </tr>
  <tr>
    <td>テストタイトル</td>
    <td>テストボディ</td>
  </tr>
</table>
<h2>New book</h2>
<%= form_with model:@book,url:'/books',local:true do |f| %>
<h4>Title</h4>
<%= f.text_field :title %>
<h4>Body</h4>
<%= f.text_area :body %>
<%= f.submit 'Create Book' %>
<% end %>